{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/game.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/gameScene.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/titleScene.js"
    }
  ]
}
