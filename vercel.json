{
  "version": 2,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "src/game.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/gameScene.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/titleScene.js"
    }
  ]
}
